			},
			{
				"command": "gif.deal",
				"title": "GIF Deal"
			},
			{
				"command": "gif.eww",
				"title": "GIF Eww"
			},
			{
				"command": "gif.facepalm",
				"title": "GIF Facepalm"
			},
			{
