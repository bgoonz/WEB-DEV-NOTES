			},
			{
				"command": "gif.mic",
				"title": "GIF Mic Drop"
			},
			{
				"command": "gif.please",
				"title": "GIF Please"
			},
			{
				"command": "gif.oops",
				"title": "GIF Oops"
			},
			{
				"command": "gif.seriously",
