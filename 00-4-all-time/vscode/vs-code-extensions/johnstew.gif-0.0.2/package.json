{
	"name": "gif",
	"displayName": "GIF",
	"description": "Easily grab a gif from within VS Code to add your to docs, presentations, etc.",
	"version": "0.0.2",
	"publisher": "johnstew",
	"engines": {
		"vscode": "^1.11.0"
	},
	"galleryBanner": {
		"color": "#5c2d91",
		"theme": "dark"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:gif.trending",
		"onCommand:gif.agree",
		"onCommand:gif.awww",
		"onCommand:gif.dance",
		"onCommand:gif.deal",
		"onCommand:gif.eww",
		"onCommand:gif.facepalm",
		"onCommand:gif.fist",
		"onCommand:gif.gl",
		"onCommand:gif.bye",
		"onCommand:gif.hug",
		"onCommand:gif.idk",
		"onCommand:gif.no",
		"onCommand:gif.omg",
		"onCommand:gif.mic",
		"onCommand:gif.please",
		"onCommand:gif.oops",
		"onCommand:gif.seriously",
		"onCommand:gif.shocked",
		"onCommand:gif.shrug",
		"onCommand:gif.sorry",
		"onCommand:gif.thanks",
		"onCommand:gif.yes"
	],
	"main": "./out/src/main",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "GIF configuration",
			"properties": {
				"gif.rating": {
					"type": "string",
					"default": "pg-13",
					"description": "Specify the rating you would like to use for GIF results."
				}
			}
		},
		"commands": [
			{
				"command": "gif.trending",
				"title": "GIF Trending"
			},
			{
