{
	"name": "markmap-vscode",
	"version": "0.0.6",
	"description": "Visualize your markdown in VSCode",
	"author": "Gerald <gera2ld@live.com>",
	"license": "MIT",
	"displayName": "Markmap",
	"publisher": "gera2ld",
	"icon": "assets/favicon.png",
	"engines": {
		"vscode": "^1.51.0"
	},
	"categories": [
		"Visualization"
	],
	"keywords": [
		"markmap",
		"mindmap",
		"markdown"
	],
	"activationEvents": [
		"onCustomEditor:markmap-vscode.markmap"
	],
	"contributes": {
		"customEditors": [
			{
				"viewType": "markmap-vscode.markmap",
				"displayName": "Markmap",
				"selector": [
					{
						"filenamePattern": "*.mm.md"
					}
				],
				"priority": "default"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn lint && yarn build",
		"build": "gulp clean && gulp build",
		"lint": "eslint --ext .ts,.tsx .",
		"watch": "gulp dev",
		"transform": "locky yarn yarn"
	},
	"repository": "https://github.com/gera2ld/markmap-vscode",
	"main": "dist/extension.js",
	"files": [
		"dist"
	],
	"devDependencies": {
		"@babel/runtime": "^7.12.5",
		"@gera2ld/locky": "^0.1.0",
		"@gera2ld/plaid": "~2.1.6",
		"@gera2ld/plaid-common-ts": "~2.1.5",
		"@gera2ld/plaid-rollup": "~2.1.3",
		"@types/glob": "^7.1.3",
		"@types/lodash.debounce": "^4.0.6",
		"@types/node": "^14.14.10",
		"@types/vscode": "^1.51.0",
		"del": "^6.0.0",
		"fancy-log": "^1.3.3",
		"gulp": "^4.0.2",
		"gulp-concat": "^2.6.1",
		"husky": "^4.3.0",
		"lodash.debounce": "^4.0.8",
		"markmap-lib": "^0.11.0",
		"markmap-toolbar": "^0.1.4"
	},
	"husky": {
		"hooks": {
			"pre-push": "yarn lint",
			"pre-commit": "yarn transform -t"
		}
	},
	"__metadata": {
		"id": "2392d4a0-3976-49ea-af95-7001abb8c75a",
		"publisherId": "4e3f72cb-a32e-4f0b-bd61-4f5f85efe190",
		"publisherDisplayName": "Gerald"
	}
}